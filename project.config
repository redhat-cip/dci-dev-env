[project]
	description = Dci Developement environment
[access "refs/*"]
  read = group dci-dev-env-core
  owner = group dci-dev-env-ptl
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group dci-dev-env-core
	label-Code-Review = -2..+2 group dci-dev-env-ptl
	label-Verified = -2..+2 group dci-dev-env-ptl
	label-Workflow = -1..+1 group dci-dev-env-core
	label-Workflow = -1..+1 group dci-dev-env-ptl
	label-Workflow = -1..+0 group Registered Users
	submit = group dci-dev-env-ptl
	read = group dci-dev-env-core
    read = group Registered Users
[access "refs/meta/config"]
    read = group dci-dev-env-core
    read = group Registered Users
[receive]
	requireChangeId = true
[submit]
	mergeContent = false
	action = fast forward only
